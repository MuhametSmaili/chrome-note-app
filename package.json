{
  "name": "note-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack --watch --progress --config ./webpack/webpack.dev.js",
    "build": "NODE_ENV=production webpack --watch --progress --config ./webpack/webpack.prod.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "husky install && chmod ug+x .husky/*",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix"
  },
  "lint-staged": {
    "**/*.(ts|tsx|js|json|svg|css)": [
      "eslint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tiptap/extension-image": "2.0.0-beta.30",
    "@tiptap/extension-link": "2.0.0-beta.43",
    "@tiptap/extension-underline": "2.0.0-beta.25",
    "@tiptap/react": "2.0.0-beta.114",
    "@tiptap/starter-kit": "2.0.0-beta.191",
    "clsx": "1.2.1",
    "html-to-pdfmake": "2.4.5",
    "lodash.debounce": "4.0.8",
    "pdfmake": "0.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.2",
    "tesseract.js": "2.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@svgr/webpack": "6.3.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@types/chrome": "0.0.193",
    "@types/html-to-pdfmake": "2.1.1",
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "@vitejs/plugin-react": "2.0.0",
    "autoprefixer": "10.4.7",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "global-jsdom": "8.5.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "style-loader": "3.3.1",
    "tailwindcss": "3.1.6",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "vitest": "0.19.1",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  }
}
